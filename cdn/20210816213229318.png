function Password(mima)
  key = gg.prompt({"输入脚本密码:"},nil,{'text'})
  if key ~= nil or key[1] ~= nil then
    if key[1]==mima==false then
      exit(print("错误: 密码不正确"))
    end
   else
     exit(print("错误: 密码未确认/密码未完整"))
  end
end

