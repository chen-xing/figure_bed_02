do
  local file = '/sdcard/.机器码.txt' --可以自己改
  local url = "http://www.iyuji.cn/iyuji/s/TXdQbjg3OEZRMEltdnhuQndWT3VTQT09/1627131190863764" --讯飞链接
  ::op::
  local file1 = io.open(file,'r')
  if file1 == nil then
    local random = math.random(1111111,9999999999)
    io.open(file, 'w'):write(random)
    goto op
  end
  local text = file1:read("*a")
  local tc = gg.makeRequest(url).content
  if not tc then return end
  local list = {}
  for value in tc:gmatch("【用户%d+】(%d+)【用户%d+】") do
    if #value > 0 then
      list[value] = true
    end
  end
  if list[text] == true then
    gg.alert("通过验证！")
  else
    local choice = gg.alert("您没有使用资格！\n您的验证码为:" .. text .. "\n请联系作者获取使用资格！", "复制机器码", "退出")
    if choice == 1 then
      gg.copyText(text, false)
    end
    return false
  end
end
YY='http://www.iyuji.cn/iyuji/s/VlVtUFJyd2lsNVNPZVpnV25pd0lCZz09/1628932647115494'
local yy = gg.makeRequest(YY).content-------版权川川
local yya = yy:match('【状态】(.-)【状态】')------脚本的开关状态
local yyb = yy:match('【公告】(.-)【公告】')------脚本的公告
local yyc = yy:match('【脚本】(.-)【脚本】')------你脚本的链接
local yyd = yy:match('【群号】(.-)【群号】')------这是脚本结束后的文字可以留你的反馈群
local yye = yy:match('【关闭后的公告】(.-)【关闭后的公告】')------脚本停止使用后的公告
local yyf = yy:match('【QQ】(.-)【QQ】')------这里是你的QQ
if yya=="开"then
gg.alert(yyb,"进入并复制作者QQ")
gg.copyText(yyf)
gg.toast("复制成功")
pcall(load(yyc))
os.exit(yyd) 
 end
if yya=="关"then
gg.alert(yye,"确定并复制作者QQ")
